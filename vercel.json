{
    "builds": [{ "src": "main.py", "use": "@vercel/python" }],
    "routes": [{ "src": "/.*", "dest": "main.py" }],
    "rewrites": [
        { "source": "/p/:match/:url*", "destination": "https://:match/:url*" },
        { "source": "/p/:match/:url*/", "destination": "https://:match/:url*/" }
    ],
    "regions": ["hkg1"]
}
